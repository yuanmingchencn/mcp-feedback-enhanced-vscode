{
  "name": "mcp-feedback-enhanced",
  "version": "1.2.5",
  "description": "MCP Feedback Enhanced Server - Interactive feedback collection for AI assistants",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-feedback-enhanced": "dist/index.js",
    "mcp-feedback-server": "dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "feedback",
    "cursor",
    "vscode",
    "claude"
  ],
  "author": "mcp-feedback",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuanmingchencn/mcp-feedback-enhanced-vscode"
  },
  "homepage": "https://github.com/yuanmingchencn/mcp-feedback-enhanced-vscode#readme",
  "bugs": {
    "url": "https://github.com/yuanmingchencn/mcp-feedback-enhanced-vscode/issues"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node --format=esm --target=node18 --minify --banner:js=\"import{createRequire}from'module';const require=createRequire(import.meta.url);\"",
    "build:dev": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ws": "^8.19.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
